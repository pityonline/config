[user]
	name = pityonline
	email = pityonline@gmail.com
[color]
	diff	= auto
	branch	= auto
	status	= auto
	grep	= auto
[alias]
	a	= add
	af	= add -f
	b	= branch
    bl  = blame
	ca	= commit -a
	cm	= commit -m
	cl	= clone
	co	= checkout
	d	= diff
	dc	= diff --cached
	ds	= diff --stat
	dsc	= diff --stat --cached
	l	= log
	lg	= log --graph
	lp	= log -p
	ls	= ls-files
	m	= merge
	pl	= pull
	ps	= push
    rc  = rm --cached
    rs  = reset HEAD^
    rl  = reset HEAD
	s	= status
	t	= tag
	w	= whatchanged
[github]
	user	= pityonline
[core]
	excludesfile = /Users/pity/.gitignore_global
	quotepath = false
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = simple
